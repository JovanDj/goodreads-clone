<p>
  <a class="btn btn-primary" data-toggle="collapse" href="#multiCollapseExample1" role="button" aria-expanded="false" aria-controls="multiCollapseExample1">Favorite
    books</a>
  <button class="btn btn-primary" type="button" data-toggle="collapse" data-target="#multiCollapseExample2" aria-expanded="false" aria-controls="multiCollapseExample2">To
    read
  </button>
</p>
<div class="row">
  <div class="col">
    <div class="collapse multi-collapse show" id="multiCollapseExample1">

      <% unless @favorited_books.blank? %>
        <h1>Favorite books</h1>
        <div class="row">
          <% @favorited_books.each do |favorite_book| %>

            <div class="col col-md-4 mb-5">
              <%= link_to image_tag(favorite_book.cover, class: "img-fluid img-thumbnail"), favorite_book %>

            </div>
          <% end %>
        </div>

      <% else %>

        <h1>No favorited books</h1>

      <% end %>
    </div>
  </div>
</div>
<div class="col">
  <div class="collapse multi-collapse" id="multiCollapseExample2">
    <% @readBooks.each do |book| %>
      <h1><%= book.title %></h1>

    <% end %>
  </div>
</div>
</div>

